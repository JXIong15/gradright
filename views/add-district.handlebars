<div class="col-md-6">
    <h2>Add District</h2>

    <form class="form" id="district-data">
        <div class="form-group">
            <label for="district-name">District Name:<span style="color:red">*</span></label>
            <input class="form-input" type="text" id="district-name" />
        </div>

        <div class="form-group">
            <label for="math">District Math Requirement: <span style="color:red">*</span></label>
            <input class="form-input" type="text" id="math" />
        </div>

        <div class="form-group">
            <label for="math">District English Requirement: <span style="color:red">*</span></label>
            <input class="form-input" type="text" id="english" />
        </div>

        <div class="form-group">
            <label for="math">District Science Requirement: <span style="color:red">*</span></label>
            <input class="form-input" type="text" id="science" />
        </div>

        <div class="form-group">
            <label for="math">District Social Studies Requirement: <span style="color:red">*</span></label>
            <input class="form-input" type="text" id="ss" />
        </div>

        <div class="form-group">
            <label for="math">District Physical Education Requirement: <span style="color:red">*</span></label>
            <input class="form-input" type="text" id="pe" />
        </div>

        <div class="form-group">
            <label for="math">District Health Requirement: <span style="color:red">*</span></label>
            <input class="form-input" type="text" id="health" />
        </div>

        <div class="form-group">
            <label for="math">District Electives Requirement: <span style="color:red">*</span></label>
            <input class="form-input" type="text" id="elective" />
        </div>

        <div class="form-group">
            <button class="btn btn-success" id="add-btn">Add District</button>
        </div>
        <div class="form-group">
            <a href="/district" class=" d-flex justify-content-end" style="margin: 20px"><button
                    class="btn btn-secondary">Back to
                    Districts</button></a>
        </div>
    </form>
</div>



<script src="/js/add-district.js"></script>